{
    "extends": "../tslint.json",
    "rules": {
        "quotemark": [true, "double", "jsx-double"],
        "directive-selector": [
            true,
            "attribute",
            "glk",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "glk",
            "kebab-case"
        ]
    }
}
